<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="twtour.content">

    <configlet
        title="Get Data Setup Control Panel"
        action_id="twtour.content.configlets.slideshow"
        appId="twtour.content"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@get_data_setup"
        icon_expr="string:/favicon.ico"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
