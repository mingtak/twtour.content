                        <hr>
<!--
                        <p class="lead">Mirum est notare quam littera gothica, quam nunc putamus parum claram</p>
-->
                    </div>
<tal:upcomingEvents define="catalog python:context.portal_catalog;
                            brain python:catalog(portal_type='twtour.content.tourevent',
                                                 sort_on='created', sort_order='reverse')[:4]"
                    repeat="item brain">
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    	<div class="testimonial_wrap" tal:define="oddOrEven repeat/item/number">
                            <div class="testimonial_details" tal:attributes="data-effect python:'slide-left' if oddOrEven%2 == 1 else 'slide-right'">
                                <a tal:attributes="href item/getURL" class="img-circle alignleft">
                                  <img tal:define="imageItem python:item.getObject()"
                                       tal:replace="structure imageItem/@@images/leadImageForEvent/thumb" />
                                </a>
                           		<p tal:content='item/Description'></p>
                                <div class="testimonial_meta clearfix">
                                    <div class="pull-left">
                                        <p tal:content="python:'%s - %s' % (str(item.getObject().startDate.strftime('%Y/%m/%d')), str(item.getObject().endDate.strftime('%Y/%m/%d')))">date region</p>
                                    </div>	
                                    <div class="pull-right">
                                        <div class="rating text-center">
                                          <a tal:attributes="href python:item.getObject().absolute_url()">...more</a>
                                        </div><!-- rating -->
                                    </div>	        
                                </div><!-- testimonial_desc -->                 
                            </div><!-- end testimonial_meta -->
                        </div><!-- end testimonial_wrap -->
                    </div><!-- end col-lg-6 -->
</tal:upcomingEvents>                    
                    
		</div><!-- end row -->
            </div><!-- end container -->
        </section><!-- end whitewrapper --> 


