                    <div class="media_element ImageWrapper boxes_img">
                        <a tal:attributes="href python:item.getObject().absolute_url()">

          <img tal:define="imageItem python:item.getObject()"
               tal:replace="structure imageItem/leadImageForAttractions/to_object/@@images/image/lead" />

                        </a>
<tal:getRegion define="region python:str(item.getObject().cityName.to_object.region);
                       color python:'orange' if region == 'Northern' else 'yellow' if region == 'Central' else 'blue' if region == 'Southern' else 'purple' if region == 'Eastern' else 'gery'">
                        <a tal:attributes="href python:item.getObject().absolute_url()">
                          <div tal:attributes="class string:ImageOverlayH ${color}"></div>
                        </a>
</tal:getRegion>
                        <div class="Buttons StyleSc">
                        </div> <!-- Buttons -->
                        <div class="StyleBe">
                          <a tal:attributes="href python:item.getObject().absolute_url()">
                            <h5 tal:content="python:'%s : %s' % (item.getObject().cityName.to_object.Title(), item.Title)"
                                style="color:#fff;">title</h5>
                          </a>
                        </div>
                    </div><!-- media_element -->
                </div>
</tal:attractionsList>
            </div>
</section>
